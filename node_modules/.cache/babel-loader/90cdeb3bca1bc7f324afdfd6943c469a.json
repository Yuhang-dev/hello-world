{"ast":null,"code":"import \"core-js/modules/es.number.constructor.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport moment from 'moment';\nimport { TYPE, SEVERITY } from './utils/dict';\nimport { getTicketById } from '@/api/ticket';\nexport default {\n  data: function data() {\n    return {\n      TYPE: TYPE,\n      SEVERITY: SEVERITY,\n      loading: false,\n      detailData: {}\n    };\n  },\n  created: function created() {\n    var _this = this;\n\n    //params是获取路由信息的路径参数值\n    //query是获取路由信息的?的参数值\n    var id = this.$route.params.id;\n    console.log(id);\n    this.loading = true;\n    getTicketById({\n      id: id\n    }).then(function (res) {\n      _this.detailData = res.data.data;\n      _this.loading = false; //console.log(moment(Number(this.detailData.expectedSolveDate)).format('YYYY-MM-DD hh:mm:ss'))\n    }).catch(function () {\n      _this.loading = false;\n    });\n  },\n  methods: {\n    dateForm: function dateForm(time) {\n      if (time) {\n        return moment(Number(time)).format('YYYY-MM-DD hh:mm:ss');\n      } else {\n        return '';\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiDA;AACA;AACA;AAEA;EACAA,IADA,kBACA;IACA;MACAC,UADA;MAEAC,kBAFA;MAIAC,cAJA;MAMAC;IANA;EAQA,CAVA;EAWAC,OAXA,qBAWA;IAAA;;IACA;IACA;IACA;IAEAC;IACA;IACAC;MACAC;IADA,GAEAC,IAFA,CAEA;MACA;MACA,sBAFA,CAGA;IACA,CANA,EAMAC,KANA,CAMA;MACA;IACA,CARA;EASA,CA3BA;EA4BAC;IACAC,QADA,oBACAC,IADA,EACA;MACA;QACA;MACA,CAFA,MAGA;QACA;MACA;IACA;EARA;AA5BA","names":["data","TYPE","SEVERITY","loading","detailData","created","console","getTicketById","id","then","catch","methods","dateForm","time"],"sourceRoot":"src/views/tickets","sources":["detail.vue"],"sourcesContent":["<template>\r\n  <div class=\"detail\" v-loading=\"loading\">\r\n    <h3 class=\"title\">{{ detailData.title }}</h3>\r\n    <div class=\"content\">\r\n      <el-row :gutter=\"16\">\r\n        <el-col :span=\"2\">Ticket ID:</el-col>\r\n        <el-col :span=\"12\">{{ detailData.id }}</el-col>\r\n        <el-col :span=\"4\">Reporter:</el-col>\r\n        <el-col :span=\"6\">Admin</el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"16\">\r\n        <el-col :span=\"2\">Status:</el-col>\r\n        <el-col :span=\"12\"></el-col>\r\n        <el-col :span=\"4\">Reporter date:</el-col>\r\n        <el-col :span=\"6\"></el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"16\">\r\n        <el-col :span=\"2\">Type:</el-col>\r\n        <el-col :span=\"12\">{{ TYPE[detailData.type] }}</el-col>\r\n        <el-col :span=\"4\">Expected solve date:</el-col>\r\n        <el-col :span=\"6\">{{ dateForm(detailData.expectedSolveDate) }}</el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"16\">\r\n        <el-col :span=\"2\">Severity:</el-col>\r\n        <el-col :span=\"12\">{{ SEVERITY[detailData.severity] }}</el-col>\r\n        <el-col :span=\"4\">Assignee:</el-col>\r\n        <el-col :span=\"6\">{{ detailData.assignee }}</el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"16\">\r\n        <el-col :span=\"2\">SN:</el-col>\r\n        <el-col :span=\"12\">{{ detailData.sn }}</el-col>\r\n        <el-col :span=\"4\">Copied:</el-col>\r\n        <el-col :span=\"6\">\r\n          {{ detailData.copiedAuto }}<br/>\r\n          {{ detailData.copiedManual ? detailData.copiedManual.join(', ') : '' }}\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"16\">\r\n        <el-col :span=\"2\">Description:</el-col>\r\n        <el-col :span=\"24\" v-html=\"detailData.description\"></el-col>\r\n      </el-row>\r\n    </div>\r\n    <div class=\"Replies\">\r\n      <h3 class=\"title\">Replies</h3>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport moment from 'moment';\r\nimport {TYPE, SEVERITY} from './utils/dict';\r\nimport {getTicketById} from '@/api/ticket';\r\n\r\nexport default {\r\n  data(){\r\n    return {\r\n      TYPE,\r\n      SEVERITY,\r\n\r\n      loading: false,\r\n\r\n      detailData: {}\r\n    }\r\n  },\r\n  created(){\r\n    //params是获取路由信息的路径参数值\r\n    //query是获取路由信息的?的参数值\r\n    var id = this.$route.params.id;\r\n    \r\n    console.log(id)\r\n    this.loading = true;\r\n    getTicketById({\r\n      id\r\n    }).then(res =>{\r\n      this.detailData = res.data.data;\r\n      this.loading = false;\r\n      //console.log(moment(Number(this.detailData.expectedSolveDate)).format('YYYY-MM-DD hh:mm:ss'))\r\n    }).catch(()=>{\r\n      this.loading = false;\r\n    });\r\n  },\r\n  methods: {\r\n    dateForm(time){\r\n      if(time){\r\n        return moment(Number(time)).format('YYYY-MM-DD hh:mm:ss');\r\n      }\r\n      else{\r\n        return ''\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep.detail{\r\n  box-sizing: border-box;\r\n  padding: 20px 32px 28px;\r\n  width: 1200px;\r\n  margin: 15px auto 0;\r\n  background: #fff;\r\n  border: 1px solid rgba(0,0,0,0.15);\r\n  .title{\r\n    font-weight: normal;\r\n    font-size: 22px;\r\n    color: #5A5A5A;\r\n    line-height: 32px;\r\n    padding: 20px 0;\r\n    border-bottom: 1px solid #D6D6D6;\r\n  }\r\n  .content{\r\n    padding: 5px 32px 10px;\r\n    background: #F5F5F5;\r\n    .el-row{\r\n      margin-top: 14px;\r\n    }\r\n    .el-col{\r\n      min-height: 22px;\r\n      line-height: 22px;\r\n      &.el-col-12{ text-align: left;}\r\n    }\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}